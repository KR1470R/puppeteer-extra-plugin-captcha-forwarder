{
  "name": "puppeteer-extra-plugin-captcha-forwarder",
  "version": "0.0.2",
  "description": "Fork of puppeteer-extra-plugin-recaptcha, captcha-forwarder provider added",
  "author": "KR1470R",
  "license": "ISC",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "type": "module",
  "scripts": {
    "clean": "rimraf dist/*",
    "tscheck": "tsc --pretty --noEmit",
    "prebuild": "run-s clean",
    "build": "run-s build:tsc build:rollup ambient-dts",
    "build:tsc": "tsc --project tsconfig.json --module commonjs",
    "build:rollup": "rollup -c rollup.config.cjs",
    "docs": "node -e 0",
    "predocs2": "rimraf docs/*",
    "docs2": "typedoc --module commonjs --readme none --target ES6 --theme markdown --excludeNotExported --excludeExternals --excludePrivate --out docs --mode file src/index.ts",
    "test": "ava -v --config ava.config-ts.js",
    "pretest-ci": "run-s build",
    "test-ci": "ava --fail-fast --concurrency 2 -v",
    "ambient-dts": "run-s ambient-dts-copy ambient-dts-fix-path",
    "ambient-dts-copy": "copyfiles -u 1 \"src/**/*.d.ts\" dist",
    "ambient-dts-fix-path": "replace-in-files --string='/// <reference path=\"../src/' --replacement='/// <reference path=\"../dist/' 'dist/**/*.d.ts'"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^6.1.0",
    "@types/debug": "^4.1.5",
    "@types/node": "^20.19.25",
    "@types/puppeteer": "*",
    "ava": "2.4.0",
    "copyfiles": "^2.1.1",
    "npm-run-all": "^4.1.5",
    "puppeteer": "9",
    "puppeteer-extra": "^3.3.6",
    "replace-in-files-cli": "^0.3.1",
    "rimraf": "^3.0.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.25.2",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "merge-deep": "^3.0.2",
    "playwright-core": "^1.56.1",
    "puppeteer-extra-plugin": "^3.2.3"
  },
  "peerDependencies": {
    "playwright-extra": "*",
    "puppeteer-extra": "*"
  },
  "peerDependenciesMeta": {
    "puppeteer-extra": {
      "optional": true
    },
    "playwright-extra": {
      "optional": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KR1470R/puppeteer-extra-plugin-captcha-forwarder.git"
  },
  "keywords": [
    "captcha",
    "recaptcha",
    "forward",
    "proxy",
    "solver",
    "2captcha",
    "puppeteer",
    "captcha-forwarder"
  ],
  "bugs": {
    "url": "https://github.com/KR1470R/puppeteer-extra-plugin-captcha-forwarder/issues"
  },
  "homepage": "https://github.com/KR1470R/puppeteer-extra-plugin-captcha-forwarder#readme"
}
